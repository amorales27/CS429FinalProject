<tr style="height:1px;text-align:center"><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td rowspan="2" style="border-right:1px solid black;height:2em;width:1em"></td><td rowspan="2" style="height:2em;width:1em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="6" rowspan="2" style="border:0px solid black;padding:0.2em"><i><span style="background:#F8D4F8"><a href="/wiki/Duke_of_Lancaster" title="Duke of Lancaster">Duke of Lancaster</a></span>, 1351</i></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td rowspan="2" style="border-right:1px solid black;height:2em;width:1em"></td><td rowspan="2" style="height:2em;width:1em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><